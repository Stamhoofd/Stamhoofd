{
    "name": "@stamhoofd/models",
    "version": "1.0.0",
    "private": true,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "license": "UNLICENCED",
    "sideEffects": false,
    "files": [
        "src"
    ],
    "scripts": {
        "build": "rm -rf ./dist/migrations && tsc -b && mkdir -p ./dist/migrations && cp ./src/migrations/*.sql ./dist/migrations && cp -R ./src/migrations/data ./dist/migrations",
        "build:full": "rm -rf ./dist && yarn build",
        "cp-migrations": "yarn build && cp ./dist/migrations/*"
    },
    "peerDependencies": {
        "@stamhoofd/crypto": "*",
        "@stamhoofd/email": "*",
        "@stamhoofd/queues": "*"
    }
}

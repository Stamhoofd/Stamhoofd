{
	"appId": "com.stamhoofd.stamhoofd",
	"appName": "Stamhoofd",
	"webDir": "dist",
	"bundledWebRuntime": false,
	"loggingBehavior": "none",
	"hideLogs": true,
	"plugins": {
		"Keyboard": {
			"resize": "none"
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		}
	},
	"server": {
		"url": "https://dashboard.stamhoofd.dev",
		"cleartext": true
	}
}

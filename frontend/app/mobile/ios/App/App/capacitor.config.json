{
	"appId": "be.ksa.ravot",
	"appName": "Ravot",
	"webDir": "dist",
	"loggingBehavior": "none",
	"plugins": {
		"Keyboard": {
			"resize": "none"
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"CapacitorUpdater": {
			"autoUpdate": false
		}
	},
	"server": {
		"hostname": "ravot.ksa.be",
		"androidScheme": "https"
	},
	"packageClassList": [
		"HttpPlugin",
		"AppPlugin",
		"AppLauncherPlugin",
		"FilesystemPlugin",
		"HapticsPlugin",
		"KeyboardPlugin",
		"PreferencesPlugin",
		"SharePlugin",
		"StatusBarPlugin",
		"CapacitorUpdaterPlugin",
		"CapacitorRateAppPlugin"
	]
}

@font-face {
font-family: "{{fontName}}";
font-display: block;
src: {{{src}}};
}

.icon:before {
font-family: {{fontName}} !important;
font-style: normal;
font-weight: normal !important;
}

{{#each codepoints}}
.icon.{{@key}}:before{
content: "\\{{this}}";
}
{{/each}}

{{#each box_icons}}
.{{@key}}-box:before {
content: "\\{{this}}";
}
{{/each}}

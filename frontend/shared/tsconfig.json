{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "strict": false,
        "jsx": "preserve",
        "importHelpers": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "baseUrl": ".",
        "types": ["webpack-env"],
        "lib": ["esnext", "dom", "dom.iterable", "scripthost"]
    },
    "include": ["**/*.ts", "**/*.tsx", "**/*.vue"],
    "exclude": ["node_modules"]
}
